#include<iostream>
#include<cstdio>
int main()
{
char employee;
double salary,sold,total;
scanf("%s %lf %lf",&employee,&salary,&sold);
total=salary+(sold*15)/100;
printf("TOTAL = R$ %.2f\n",total);
return 0;
}
